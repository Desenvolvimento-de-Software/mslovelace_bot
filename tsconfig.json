{
    "compilerOptions": {
        "target": "es6",
        "module": "ESNEXT",
        "rootDir": "./src",
        "moduleResolution": "node",
        "baseUrl": "./src",
        "outDir": "./dist",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "noImplicitAny": true,
        "skipLibCheck": true
    }
}
